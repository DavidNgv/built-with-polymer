{
  "name": "builtwithpolymer",
  "root": "./build",
  "clean_urls": ["/!(bower_components|components)/**"],
  "error_page": "error.html",
  "forms": {
    "submit": {
      "subject": "Submission from {{name}}",
      "from": "Built with Polymer <hello@builtwithpolymer.org>",
      "replyTo": "{{name}} <{{email}}>",
      "to": "Built with Polymer <hello@builtwithpolymer.org>",
      "text": "{{project}}\n{{url}}\n\n{{desc}}",
      "success": "/submit-success",
      "failure": "/submit-failure"
    }
  }
}